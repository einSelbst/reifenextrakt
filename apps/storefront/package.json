{
  "name": "storefront",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev:wundergraph": "wunderctl up --debug",
    "dev:next": "next dev -p 3002",
    "dev": "pnpm dev:wundergraph & pnpm dev:next",
    "build:wundergraph": "wunderctl generate --debug",
    "build:next": "next build",
    "build": "pnpm build:wundergraph & pnpm build:next",
    "start": "next start",
    "clean": "rm -rf .next && rm -rf generated",
    "lint:original": "TIMING=1 next lint",
    "lint": "tsc --noEmit && TIMING=1 eslint . --format=pretty --cache"
  },
  "dependencies": {
    "@tanstack/react-query": "4.22.0",
    "@wundergraph/nextjs": "0.8.0",
    "@wundergraph/react-query": "0.3.0",
    "@wundergraph/sdk": "0.131.0",
    "graphql": "16.6.0",
    "logger": "workspace:*",
    "next": "latest",
    "next-auth": "latest",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ui": "workspace:*"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^17.0.12",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "eslint-config-custom": "workspace:*",
    "tsconfig": "workspace:*",
    "typescript": "^4.8.3"
  }
}
